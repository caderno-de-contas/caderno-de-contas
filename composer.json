    "autoload": {
        "psr-4": {
            "Domain\\": "src/domain/",
            "Tests\\": "tests/"
        }
    },
    "require": {
        "twig/twig": "^3.21",
        "vlucas/phpdotenv": "^5.6",
        "php-di/php-di": "^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "pestphp/pest": "^3.8.2"
    },
    "scripts": {
        "phpstan": [
            "./vendor/bin/phpstan analyse"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
