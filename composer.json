{
    "name": "caderno-de-contas/caderno-de-contas",
    "authors": [
        {
            "name": "gilglecio oliveira",
            "email": "gilglecio_765@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Domain\\": "src/domain/",
            "Tests\\": "tests/"
        }
    },
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "php-di/php-di": "^7.0",
        "slim/slim": "4.*",
        "slim/twig-view": "^3.4",
        "slim/psr7": "^1.7",
        "monolog/monolog": "^3.9"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "pestphp/pest": "^3.8.2"
    },
    "scripts": {
        "phpstan": [
            "./vendor/bin/phpstan analyse"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
